{
    "name": "itinerisltd/gf-sagepay",
    "type": "wordpress-plugin",
    "description": "Gravity Forms Add-on for SagePay",
    "license": "proprietary",
    "require": {
        "php": "^7.1",
        "league/omnipay": "^3.0",
        "omnipay/sagepay": "^3.0"
    },
    "require-dev": {
        "automattic/vipwpcs": "dev-master",
        "itinerisltd/preflight-command": "^0.4.0",
        "roave/security-advisories": "dev-master",
        "wp-coding-standards/wpcs": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Itineris\\SagePay\\": "src/"
        }
    },
    "scripts": {
        "check-style": "phpcs --standard=ruleset.xml --colors -p -s"
    },
    "config": {
        "sort-packages": true
    }
}
